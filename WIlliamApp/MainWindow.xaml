<Window x:Class="WIlliamApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WIlliamApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        SizeToContent="WidthAndHeight">
    <StackPanel>
        <Label>Jämför två tal</Label>

        <StackPanel Orientation="Horizontal">
            <Label Margin="10" Width="70">Tal 1: </Label>
            <TextBox Margin="10" Width="150" Name="txbTal1"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Margin="10" Width="70">Tal 2: </Label>
            <TextBox Margin="10" Width="150" Name="txbTal2"></TextBox>
        </StackPanel>
            
        <StackPanel Orientation="Horizontal" Margin="10">
            <Button Click="KlickJämför" Margin="10" Padding="5" Width="50">Jämför</Button>
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Margin="10" Width="70">Resultat: </Label>
            <TextBox Margin="10" Width="150" Name="txbResultat"></TextBox>
        </StackPanel>
    </StackPanel>
</Window>
